<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.2.xsd">
	
	<import resource="application-batch.xml"/>

	<bean id="creditBill" class="com.daniel.springBatch.model.pojo.CreditBill">
		<property name="accountID" value="4047390012345678"></property>
		<property name="name" value="tom"></property>
		<property name="account" value="100.00"></property>
		<property name="date" value="2013-2-2 12:00:08"></property>
		<property name="address" value="Hunan road"></property>
	</bean>
	
		
	
</beans>
